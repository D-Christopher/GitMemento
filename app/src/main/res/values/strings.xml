    <string name="app_name">Git Memento</string>

    <string name="header_name">Git Memento</string>
    <string name="header_version">Version 1.0.0</string>

    <string name="openDrawer">Ouverture</string>
    <string name="closeDrawer">Fermeture</string>

    <!-- Menu -->
    <string name="action_setup_help">Aide et installation</string>
    <string name="action_create">Création</string>
    <string name="action_show">Affichage</string>
    <string name="action_revert">Revenir</string>
